<div class="ui segment" id="windChart">
  <div id="map"></div>
  <script>
    const map = L.map('map').setView([53.450, -8.50], 7);
    const attribution =
      '&copy; <a href="https://wwwlopenstreetmap.org/copyright">OpenStreetMap</a> contributors'
    const tileUrl = 'https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png';
    const tiles = L.tileLayer(tileUrl, {attribution});
    tiles.addTo(map);
    //const marker = L.marker([52.16, -7.15]).addTo(map);
    {{#each pins}}
      L.marker([{{pinLat}}, {{pinLong}}]).addTo(map);
    {{/each}}
  </script>

</div>